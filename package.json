{
  "name": "vue-express-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "------------------------------------------ dev ------------------------------------------": "",
    "watch": "npm run webpack:watch:server & npm run watch:server",
    "------------------------------------------ misc -----------------------------------------": "",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack:watch:server": "NODE_ENV=development webpack --watch --config ./build/webpack.server.config.js",
    "watch:server": "NODE_ENV=development nodemon ./build/dev-server.js --watch ./www/",
    "webpack:watch:client": "NODE_ENV=development webpack --watch --config ./build/webpack.client.config.js",
    "build": "NODE_ENV=production webpack --config ./build/webpack.server.config.js",
    "clean": "npm cache clean -f && rm -rf ./node_modules"
  },
  "author": "RyosukeCla",
  "license": "MIT",
  "description": "vue express boilerplate with webpack",
  "whiteListedModules": [
    "vue"
  ],
  "dependencies": {
    "express": "4.15.4",
    "mongoose": "4.11.10",
    "pug": "2.0.0-rc.4",
    "redis": "2.8.0",
    "vue": "2.4.2",
    "webpack": "3.5.6"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-plugin-istanbul": "4.1.4",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2017": "6.24.1",
    "babel-register": "6.26.0",
    "babili-webpack-plugin": "0.1.2",
    "chokidar": "1.7.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.28.7",
    "eslint": "4.6.1",
    "eslint-config-standard": "10.2.1",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-html": "3.2.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
