{
  "name": "vue-express-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "------------------------------------------ dev -------------------------------------------": "",
    "dev": "npm run webpack:watch:server & npm run watch:server",
    "------------------------------------------ test ------------------------------------------": "",
    "test": "npm run test:e2e && npm run test:unit",
    "test:e2e": "DEBUG=nightmare xvfb-run npm run test:e2e:raw",
    "test:unit": "NODE_ENV=development karma start ./test/unit/karma.config.js --single-run",
    "------------------------------------------ production ------------------------------------": "",
    "build": "npm run build:server && npm run build:client",
    "start": "node ./www/server.js",
    "------------------------------------------ misc ------------------------------------------": "",
    "test:e2e:raw": "BABEL_ENV=test mocha --compilers js:babel-register ./test/e2e/**/*.spec.js --timeout 10000",
    "webpack:watch:server": "NODE_ENV=development webpack --watch --config ./build/webpack.server.config.js",
    "watch:server": "NODE_ENV=development nodemon ./build/dev-server.js --watch ./www/",
    "webpack:watch:client": "NODE_ENV=development webpack --watch --config ./build/webpack.client.config.js",
    "build:server": "NODE_ENV=production webpack --config ./build/webpack.server.config.js",
    "build:client": "NODE_ENV=production webpack --config ./build/webpack.client.config.js",
    "clean": "npm cache clean -f && rm -rf ./node_modules ./assets ./www"
  },
  "author": "RyosukeCla",
  "license": "MIT",
  "description": "vue express boilerplate with webpack",
  "whiteListedModules": [
    "vue",
    "socket.io",
    "axios",
    "vue-router"
  ],
  "dependencies": {
    "axios": "0.16.2",
    "body-parser": "1.17.2",
    "cookie-parser": "1.4.3",
    "express": "4.15.4",
    "mongoose": "4.11.10",
    "morgan": "1.8.2",
    "pug": "2.0.0-rc.4",
    "redis": "2.8.0",
    "serve-favicon": "2.4.3",
    "socket.io": "2.0.3",
    "vue": "2.4.2",
    "vue-router": "2.7.0",
    "webpack": "3.5.6"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-plugin-istanbul": "4.1.4",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-register": "6.26.0",
    "babili-webpack-plugin": "0.1.2",
    "chai": "4.1.2",
    "chokidar": "1.7.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.28.7",
    "eslint": "4.6.1",
    "eslint-config-standard": "10.2.1",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-html": "3.2.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.30.1",
    "inject-loader": "3.0.1",
    "is-docker": "^1.1.0",
    "json-loader": "0.5.7",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "mocha": "3.5.1",
    "nightmare": "2.10.0",
    "node-sass": "4.5.3",
    "nodemon": "1.12.0",
    "pug-html-loader": "1.1.5",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "vue-html-loader": "1.2.4",
    "vue-loader": "13.0.4",
    "vue-style-loader": "3.0.1",
    "vue-template-compiler": "2.4.2",
    "webpack": "3.5.6",
    "webpack-dev-middleware": "1.12.0",
    "webpack-dev-server": "2.7.1",
    "webpack-hot-middleware": "2.19.1",
    "webpack-node-externals": "1.6.0"
  }
}
